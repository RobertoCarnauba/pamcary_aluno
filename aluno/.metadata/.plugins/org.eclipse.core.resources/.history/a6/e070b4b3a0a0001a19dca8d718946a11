package com.pamcary.aluno.controller.dto;

public class AlunoDto {
	

	private Long id;
	private String nome;
	private int idade;
	
	
	public Long getId() {
		return id;
	}
	public String getNome() {
		return nome;
	}
	public int getIdade() {
		return idade;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	public void setIdade(int idade) {
		this.idade = idade;
	}

}
